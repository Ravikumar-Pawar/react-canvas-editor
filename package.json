{
  "name": "document-editor",
  "version": "1.0.0",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "typings": "./dist/index.d.ts",
  "main": "dist/index.js",
  "scripts": {
    "build-lib": "rm -rf dist && npm cache clean --force && rollup -c",
    "publish-locally": "npx yalc publish",
    "build": "npm link  @mindfiredigital/canvas-editor && npm run build-lib && npm run publish-locally",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.22.15",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-typescript": "^8.3.3",
    "@storybook/addon-essentials": "^7.4.6",
    "@storybook/addon-interactions": "^7.4.6",
    "@storybook/addon-links": "^7.4.6",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.4.6",
    "@storybook/react": "^7.4.6",
    "@storybook/react-webpack5": "^7.4.6",
    "@storybook/testing-library": "^0.2.2",
    "@svgr/webpack": "^8.0.1",
    "@types/node": "^16.18.34",
    "@types/react": "^18.2.8",
    "@types/react-color": "^3.0.6",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "babel-loader": "^9.1.2",
    "css-loader": "^6.8.1",
    "eslint": "^8.42.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "postcss-loader": "^7.3.2",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-scss": "^4.0.0",
    "rollup-plugin-svg": "^2.0.0",
    "sass": "^1.68.0",
    "storybook": "^7.4.6",
    "style-loader": "^3.3.3",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@grammarly/editor-sdk-react": "^2.5.1",
    "@mindfiredigital/canvas-editor": "^1.0.3",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.5",
    "@reduxjs/toolkit": "^1.9.5",
    "@storybook/preset-scss": "^1.0.3",
    "axios": "^1.4.0",
    "html2canvas": "^1.4.1",
    "html2pdf.js": "^0.9.0",
    "jodit-react": "^1.3.39",
    "js-base64": "^3.7.5",
    "jspdf": "^2.5.1",
    "react-color": "^2.19.3",
    "react-redux": "^8.1.1",
    "react-router-dom": "^6.12.0",
    "react-scripts": "^5.0.1",
    "typescript": "^4.9.5",
    "url-loader": "^4.1.1"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
